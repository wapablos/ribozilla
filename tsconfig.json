{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "outDir": "dist",
    "removeComments": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": false,
    "moduleResolution": "node",
    "baseUrl": ".",
    "types": [
      "./src/typings"
    ],
    "paths": {
      "@fonts/*":[
        "public/fonts/*"
      ],
      "@extensions/*":[
        "extensions/*"
      ],
      "@constants/*":[
        "src/constants/*"
      ],
      "@components/*":[
        "src/components/*"
      ],
      "@screens/*": [
        "src/screens/*"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@store": [
        "src/store/."
      ]
    },
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules/**/*"
  ]
}
